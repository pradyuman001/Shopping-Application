<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.admin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Admin"
        tools:targetApi="31">
        <activity
            android:name=".activity.cartActivity.view.cartActivity"
            android:exported="false" />
        <activity
            android:name=".activity.searchActivity.view.searchActivity"
            android:exported="false" />
        <activity
            android:name=".activity.updateActivity.view.updateActivity"
            android:exported="false" />
        <activity
            android:name=".activity.insertActivity.view.insertActivity"
            android:exported="false" />
        <activity
            android:name=".activity.productActivity.view.productActivity"
            android:exported="true"
            android:theme="@style/Theme.Admin.NoActionBar" />
        <activity
            android:name=".activity.loginActivity.view.loginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.mainActivity.view.MainActivity"
            android:exported="true" />
    </application>

</manifest>